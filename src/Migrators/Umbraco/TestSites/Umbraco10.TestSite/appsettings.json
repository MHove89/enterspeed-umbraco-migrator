{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "6da041bb-0d79-44c7-a01e-8572ec25e891",
        "SanitizeTinyMce": true
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "EnterspeedConfiguration": {
    "ApiKey": "environment-7514135c-f737-48b8-a7f6-aae22ff807ac",
    "NavigationHandle": "navigation-en-us",
    "MigrationPageMetaData": "migrationPageMetaData",
    "ComponentPropertyTypeKeys": [
      "rte",
      "embed",
      "headline",
      "macro",
      "media",
      "quote"
    ]
  },
  "UmbracoMigrationConfiguration": {
    "RootDocType": "home",
    "ContentPropertyAlias": "blockListContent",
    "CompositionKeys": [
      "basePage",
      "footer"
    ],
    "StartIds": {
      "SourceId": "",
      "TargetParentId": 0
    }
  }
}